<?xml version="1.0" encoding="utf-8"?>
<ContentPage  xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:warszaty" x:Class="warszaty.warszatyPage">
	<StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" >
		<Entry  x:Name="LoginEntry" Placeholder="Login" />
		<Entry  x:Name="PasswordEntry" Placeholder="Password" IsPassword="true" />
		<Button Text="Log in" Clicked="Login_Clicked"/>
	</StackLayout>
</ContentPage>
